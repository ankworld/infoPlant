<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import java.net.URL?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.mainMenuController">
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="267.0" prefWidth="442.0" VBox.vgrow="ALWAYS">
        <Label alignment="CENTER" layoutX="243.0" layoutY="14.0" style="&#10;" text="Main Menu"
               textAlignment="CENTER" textFill="#4d4b4b" wrapText="false">
            <font>
                <Font name="System Bold" size="30.0"/>
            </font>
        </Label>
        <Button fx:id="btnChoose" layoutX="60.0" layoutY="83.0" mnemonicParsing="false" prefHeight="44.0"
                prefWidth="107.0" text="เลือกไฟล์"/>
        <Button fx:id="btnMerge" layoutX="60.0" layoutY="174.0" mnemonicParsing="false" prefHeight="44.0"
                prefWidth="107.0" text="รวมไฟล์"/>
        <Button fx:id="btnAdd" layoutX="60.0" layoutY="313.0" mnemonicParsing="false" prefHeight="44.0"
                prefWidth="107.0" text="เพิ่ม/แก้ไข"/>
        <Button fx:id="btnView" layoutX="212.0" layoutY="312.0" mnemonicParsing="false" prefHeight="44.0"
                prefWidth="107.0" text="ดูข้อมูล"/>
        <Button fx:id="btnExit" layoutX="490.0" layoutY="313.0" mnemonicParsing="false" prefHeight="44.0"
                prefWidth="107.0" text="ออก"/>
        <Label fx:id="lbText" layoutX="243.0" layoutY="90.0" prefHeight="29.0" prefWidth="300"
               text="กรุณาเลือกไฟล์"/>
        <Separator layoutX="21.0" layoutY="289.0" prefHeight="4.0" prefWidth="609.0"/>

    </AnchorPane>
</VBox>
